<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.database_gui.GUI_Classes.StudentController">
   <children>
      <TableView fx:id="studentTable" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="StudentID" />
          <TableColumn fx:id="NameCol" prefWidth="75.0" text="Student Name" />
            <TableColumn fx:id="EmailCol" prefWidth="75.0" text="Email" />
            <TableColumn fx:id="DeptCol" prefWidth="75.0" text="Department" />
            <TableColumn fx:id="RouteCol" prefWidth="75.0" text="RouteID" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</VBox>
