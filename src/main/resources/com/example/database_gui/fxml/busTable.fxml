<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.database_gui.GUI_Classes.BusController">
   <children>
      <TableView fx:id="busTable" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="BusID" />
          <TableColumn fx:id="PlateCol" prefWidth="75.0" text="Plate Number" />
            <TableColumn fx:id="ModelCol" prefWidth="75.0" text="Model" />
            <TableColumn fx:id="CapCol" prefWidth="75.0" text="Capacity" />
            <TableColumn fx:id="RouteCol" prefWidth="75.0" text="RouteID" />
        </columns>
      </TableView>
      <HBox spacing="520.0">
         <children>
            <Button mnemonicParsing="false" onAction="#goBack" text="Back" />
            <Button mnemonicParsing="false" onAction="#handleEditButton" text="Edit" />
         </children>
      </HBox>
   </children>
</VBox>
