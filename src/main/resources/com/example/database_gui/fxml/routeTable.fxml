<VBox xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.database_gui.GUI_Classes.RouteController">
   <children>
      <TableView fx:id="routeTable" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="552.0" prefWidth="599.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="RouteID" />
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Route Name" />
            <TableColumn fx:id="startCol" prefWidth="75.0" text="Start Location" />
            <TableColumn fx:id="endCol" prefWidth="75.0" text="End Location" />
        </columns>
      </TableView>
      <HBox spacing="520.0">
         <children>
            <Button mnemonicParsing="false" onAction="#goBack" text="Back" />
